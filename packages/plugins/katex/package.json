{
  "name": "markdown-it-katex-for-enhancer",
  "version": "3.0.2",
  "description": "Fast math support for markdown-it with KaTeX",
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --watch",
    "test": "vitest"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "keywords": [
    "markdown",
    "KaTeX",
    "math",
    "LaTeX",
    "markdown-it-plugin",
    "markdown-it",
    "markdown-it-enhancer"
  ],
  "license": "MIT",
  "dependencies": {
    "katex": "catalog:prod"
  },
  "peerDependencies": {
    "markdown-it-enhancer": "workspace:~15.0.0 || ~15.1.0"
  },
  "peerDependenciesMeta": {
    "markdown-it-enhancer": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.14.0"
}

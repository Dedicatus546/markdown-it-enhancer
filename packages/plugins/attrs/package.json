{
  "name": "markdown-it-attrs-for-enhancer",
  "version": "4.3.1020",
  "description": "Add classes, identifiers and attributes to your markdown with {} curly brackets, similar to pandoc's header attributes",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "vite build",
    "build:dev": "vite build --watch",
    "test": "vitest"
  },
  "homepage": "https://github.com/arve0/markdown-it-attrs",
  "keywords": [
    "commonmark",
    "markdown",
    "markdown-it",
    "markdown-it-plugin",
    "attributes",
    "classes",
    "ids",
    "identifiers",
    "curly brackets",
    "pandoc",
    "header_attributes"
  ],
  "peerDependencies": {
    "markdown-it-enhancer": "workspace:*"
  },
  "peerDependenciesMeta": {
    "markdown-it-enhancer": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=20"
  },
  "packageManager": "pnpm@10.14.0"
}
